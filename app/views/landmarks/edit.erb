<h2>Edit Historical Landmark</h2>
<%= @landmark.name%><br>
<%= @landmark.year_completed.to_s%>

<form action="/landmarks/<%= @landmark.id%>" method="post">
    <input id="hidden" type="hidden" name="_method" value="patch">
    Name: <input type="text" name="landmark[name]" id="name">
    Year of Completion: <input type="text" name="landmark[year_completed]" id="year_completed">
    <input type="submit" value="Edit Landmark">
</form>

<a class="button" href="/landmarks">Landmarks</a>
<a class="button" href="/">Home</a>
